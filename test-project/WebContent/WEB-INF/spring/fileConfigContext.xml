       xsi:schemaLocation=
       "http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">


	<!-- bean For BCBS SSO login -->

	<bean id="fileManager" lazy-init="true" scope="singleton"
		class="com.bcbs.saml.FileManager">
